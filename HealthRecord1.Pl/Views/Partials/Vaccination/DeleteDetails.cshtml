@model HealthRecord1.BLL.Models.VaccinationVM

<dl class="row">
    <dt class="col-sm-2">المعرف</dt>
    <dd class="col-sm-10">@Model.Id</dd>
    <dt class="col-sm-2">اسم اللقاح</dt>
    <dd class="col-sm-10">@Model.VaccineName</dd>
    <dt class="col-sm-2">الشركة المصنعة</dt>
    <dd class="col-sm-10">@Model.Manufacturer</dd>
    <dt class="col-sm-2">عدد الجرعات القياسي</dt>
    <dd class="col-sm-10">@Model.StandardDoseCount</dd>
    <dt class="col-sm-2">الوصف</dt>
    <dd class="col-sm-10">@Model.Description</dd>
    <dt class="col-sm-2">نشط</dt>
    <dd class="col-sm-10">@(Model.IsActive ? "نعم" : "لا")</dd>
</dl>
