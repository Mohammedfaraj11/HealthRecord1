@model HealthRecord1.BLL.Models.OperationVM

<dl class="row">
    <dt class="col-sm-2">المعرف</dt>
    <dd class="col-sm-10">@Model.Id</dd>
    <dt class="col-sm-2">الاسم</dt>
    <dd class="col-sm-10">@Model.Name</dd>
    <dt class="col-sm-2">التاريخ</dt>
    <dd class="col-sm-10">@Model.Date.ToString("yyyy-MM-dd HH:mm")</dd>
    <dt class="col-sm-2">تاريخ الإنشاء</dt>
    <dd class="col-sm-10">@Model.CreationDate.ToString("yyyy-MM-dd HH:mm")</dd>
    <dt class="col-sm-2">الجراح</dt>
    <dd class="col-sm-10">@Model.Surgeon</dd>
    <dt class="col-sm-2">المدة (دقائق)</dt>
    <dd class="col-sm-10">@Model.Duration</dd>
    <dt class="col-sm-2">الفريق الطبي</dt>
    <dd class="col-sm-10">@Model.MedicalTeam</dd>
    <dt class="col-sm-2">معرف المريض</dt>
    <dd class="col-sm-10">@Model.PatientId</dd>
    <dt class="col-sm-2">الملاحظات</dt>
    <dd class="col-sm-10">@Model.Notes</dd>
</dl>
